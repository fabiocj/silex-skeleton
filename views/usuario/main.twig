{% extends "layout.twig" %}

{% block content %}
<h1 style="text-align: center;">USERS</h1>
<h3 style="text-align: center;">P&aacute;ginas de Usu&aacute;rios</h3>
<div style="text-align: center; margin-bottom: 20px;"">
    <a href="/">Homepage</a>
</div>
<table>
    <thead>
        <tr>
            {% for col in cols %}
            <th>{{ col }}</th>
            {% endfor %}
        </tr>
